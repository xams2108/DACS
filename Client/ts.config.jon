{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "react-jsx", // Phải đặt thành "react-jsx" để khớp với jsxRuntime: 'automatic'
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }